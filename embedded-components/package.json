  "files": [
    "dist"
  ],
  "main": "./dist/umd/ef-components.js",
  "module": "./dist/esm/ef-components.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/esm/ef-components.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/umd/ef-components.js"
      }
    }
  },
  "scripts": {
    "__comment_build": "=== BUILD ===",
    "build": "rimraf dist && tsc && vite build",
    "build:analyze": "vite build --mode analyze",
    "__comment_dev": "=== DEVELOPMENT ===",
    "dev": "vite",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 npx storybook build --disable-telemetry",
    "__comment_test": "=== TESTING ===",
    "test": "npm run typecheck && npm run format:check && npm run lint && npm run test:unit",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "__comment_quality": "=== CODE QUALITY ===",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --cache",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx}\"",
    "__comment_security": "=== SECURITY ===",
    "audit": "yarn audit --level moderate",
    "audit:fix": "yarn audit fix",
    "__comment_analysis": "=== ANALYSIS ===",
    "analyze": "vite build --mode analyze",
    "visualize": "vite-bundle-visualizer",
    "__comment_api": "=== API GENERATION ===",
    "generate-api": "orval --config ./orval.config.cjs && yarn fix-generated-files && yarn prettify-generated-files",
    "fix-generated-files": "yarn replace '((?:\\s|\\<))(\\d\\S*)Response' '$1N$2Response' ./src/api/generated/*",
    "prettify-generated-files": "npx prettier --write './src/api/generated/*'",
    "__comment_deprecated": "=== DEPRECATED (use format/test:unit) ===",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest"
  },
